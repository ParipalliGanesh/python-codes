# cook your dish here
z=int(input())
for i in range(0,z):
    s1=input()
    s2=input()
    l=[]
    for j in range(0,len(s1)):
        a=s1[j]
        b=s2[j]
        
        if a==b:
           l.append("G")
        else:
           l.append("B")
    
    o=l[0]
    for g in range (1,len(l)):
        
        o=o+l[g]
        
    print(o)
